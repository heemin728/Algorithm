#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;
int n;

int main() {
    cin >> n;
    vector<vector<int>> arr(n, vector<int>(n));

    for(int i=0;i<n;i++) {
        for(int j=0;j<n;j++) {
            cin >> arr[i][j];
        }
    }
    
    vector<vector<int>> ex(n, vector<int>(n, 0));
    vector<vector<int>> in(n, vector<int>(n, 0));

    ex[0][0] = arr[0][0];
    in[0][0] = arr[0][0]*2;

    for(int i=0;i<n;i++) {
        for(int j=0;j<n;j++) {
            if(i>0) {
                ex[i][j] = max(ex[i][j], ex[i-1][j] + arr[i][j]);
                in[i][j] = max(in[i][j], in[i-1][j] + arr[i][j]);
                in[i][j] = max(in[i][j], ex[i-1][j] + arr[i][j] * 2);
            }
            if(j>0) {
                ex[i][j] = max(ex[i][j], ex[i][j-1] + arr[i][j]);
                in[i][j] = max(in[i][j], in[i][j-1] + arr[i][j]);
                in[i][j] = max(in[i][j], ex[i][j-1] + arr[i][j] * 2);
            }
        }
    }
    cout << in[n-1][n-1] << "\n";
    
    return 0;
}
